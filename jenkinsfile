node {
   echo 'Hello World'
   
    stage('Clone sources') {
        git url: 'https://github.com/PeterPiPer/Faffing.git/'
    }
    stages {
      stage('Stage1') {
          echo 'Stage 1'
      }

      stage('Stage2') {
          echo 'Stage 2'
      }
      
      stage('Stage3') {
          echo 'Stage 3'
      }

      stage('new pipeline') {
         steps {
            build job: 'notMyPipeline', propagate: true, wait: true, parameters: [
               string(name: 'param1', value: "value1")
            ]
         }
      }

      stage('Publish build info') {
          echo 'Done'
      }
    }
}

